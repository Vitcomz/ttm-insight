.boton{
background-color:#eeeeee; 
layer-background-color:#999999;
color:#black;
cursor:pointer;
cursor:hand;
}

.botonBlanco
{
	filter:progid:dximagetransform.microsoft.gradient
(gradienttype=0,startcolorstr="#f9f9f1",endcolorstr="#e7e8dd");
	border:solid 1px #6587A3;
	background-color:Silver;
	font-family:Verdana, Helvetica, sans-serif;
	font-size:10px;
	font-weight:normal;
	padding:1px 3px 1px 3px;
	width:80px;
	cursor:hand;
   h: expression(onmouseover=new Function("this.style.backgroundColor='#ff0';this.style.fontWeight='bold';"),
                 onmouseout=new Function("this.style.backgroundColor='Silver';this.style.fontWeight='normal';")
				 );
}

.botonBlancoLargo
{
	filter:progid:dximagetransform.microsoft.gradient
(gradienttype=0,startcolorstr="#f9f9f1",endcolorstr="#e7e8dd");
	border:solid 1px #6587A3;
	background-color:Silver;
	font-family:Verdana, Helvetica, sans-serif;
	font-size:10px;
	font-weight:normal;
	padding:1px 3px 1px 3px;
	cursor:hand;
   h: expression(onmouseover=new Function("this.style.backgroundColor='#ff0';this.style.fontWeight='bold';"),
                 onmouseout=new Function("this.style.backgroundColor='Silver';this.style.fontWeight='normal';")
				 );
}

